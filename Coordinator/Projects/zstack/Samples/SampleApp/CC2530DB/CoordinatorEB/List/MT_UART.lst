    330          /***************************************************************************************************
    331           * @fn      MT_UartProcessZAppData
    332           *
    333           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    334           *          |  1  |  2   |       1         |  1   |
    335           *
    336           *          Parses the data and determine either is SPI or just simply serial data
    337           *          then send the data to correct place (MT or APP)
    338           *
    339           * @param   port    - UART port
    340           *          event   - Event that causes the callback
    341           *
    342           *
    343           * @return  None
    344           ***************************************************************************************************/
    345          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    346          {
    347          
    348            osal_event_hdr_t  *msg_ptr;
    349            uint16 length = 0;
    350            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    351          
    352            /*
    353               If maxZAppBufferLength is 0 or larger than current length
    354               the entire length of the current buffer is returned.
    355            */
    356            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    357            {
    358              length = MT_UartMaxZAppBufLen;
    359            }
    360            else
    361            {
    362              length = rxBufLen;
    363            }
    364          
    365            /* Verify events */
    366            if (event == HAL_UART_TX_FULL)
    367            {
    368              // Do something when TX if full
    369              return;
    370            }
    371          
    372            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    373            {
    374              if ( App_TaskID )
    375              {
    376                /*
    377                   If Application is ready to receive and there is something
    378                   in the Rx buffer then send it up
    379                */
    380                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    381                {
    382                  /* Disable App flow control until it processes the current data */
    383                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    384          
    385                  /* 2 more bytes are added, 1 for CMD type, other for length */
    386                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    387                  if ( msg_ptr )
    388                  {
    389                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    390                    msg_ptr->status = length;
    391          
    392                    /* Read the data of Rx buffer */
    393                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    394          
    395                    /* Send the raw data to application...or where ever */
    396                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    397                  }
    398                }
    399              }
    400            }
    401          }
