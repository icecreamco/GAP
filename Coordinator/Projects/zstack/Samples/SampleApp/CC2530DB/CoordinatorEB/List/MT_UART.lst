    403          /***************************************************************************************************
    404           * @fn      SPIMgr_ZAppBufferLengthRegister
    405           *
    406           * @brief
    407           *
    408           * @param   maxLen - Max Length that the application wants at a time
    409           *
    410           * @return  None
    411           *
    412           ***************************************************************************************************/
    413          void MT_UartZAppBufferLengthRegister ( uint16 maxLen )
    414          {
    415            /* If the maxLen is larger than the RX buff, something is not right */
    416            if (maxLen <= MT_UART_DEFAULT_MAX_RX_BUFF)
    417              MT_UartMaxZAppBufLen = maxLen;
    418            else
    419              MT_UartMaxZAppBufLen = 1; /* default is 1 byte */
    420          }
