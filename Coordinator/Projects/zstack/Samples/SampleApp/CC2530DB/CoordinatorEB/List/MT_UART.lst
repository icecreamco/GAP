    422          /***************************************************************************************************
    423           * @fn      SPIMgr_AppFlowControl
    424           *
    425           * @brief
    426           *
    427           * @param   status - ready to send or not
    428           *
    429           * @return  None
    430           *
    431           ***************************************************************************************************/
    432          void MT_UartAppFlowControl ( bool status )
    433          {
    434          
    435            /* Make sure only update if needed */
    436            if (status != MT_UartZAppRxStatus )
    437            {
    438              MT_UartZAppRxStatus = status;
    439            }
    440          
    441            /* App is ready to read again, ProcessZAppData have to be triggered too */
    442            if (status == MT_UART_ZAPP_RX_READY)
    443            {
    444              MT_UartProcessZAppData (MT_UART_DEFAULT_PORT, HAL_UART_RX_TIMEOUT );
    445            }
