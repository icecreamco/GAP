    421          static void zdoSendStateChangeMsg(uint8 state, uint8 taskId)
    422          {
    423            osal_event_hdr_t *pMsg = (osal_event_hdr_t *)osal_msg_find(taskId, ZDO_STATE_CHANGE);
    424          
    425            if (NULL == pMsg)
    426            {
    427              if (NULL == (pMsg = (osal_event_hdr_t *)osal_msg_allocate(sizeof(osal_event_hdr_t))))
    428              {
    429                // Upon failure to notify any EndPoint of the state change, re-set the ZDO event to
    430                // try again later when more Heap may be available.
    431                osal_set_event(ZDAppTaskID, ZDO_STATE_CHANGE_EVT);
    432              }
    433              else
    434              {
    435                pMsg->event = ZDO_STATE_CHANGE;
    436                pMsg->status = state;
    437          
    438                (void)osal_msg_send(taskId, (uint8 *)pMsg);
    439              }
    440            }
    441            else
    442            {
    443              // Modify in place the status of an existing ZDO_STATE_CHANGE message to the EndPoint.
    444              pMsg->status = state;
    445            }
    446          }
